{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "app"
      }
    ],
    "functions": {
      "engines": {
        "node": "16"
      }
    }
  },

  "functions": {
    
   "source":"functions"

  },

  "emulators": {
    "hosting": {
      "port": 9000
    },
    "ui": {
      "enabled": true,
      "port": 6001
    },
    "singleProjectMode": true,
    "apphosting": {
      "port": 5002,
      "rootDirectory": "./",
      "startCommand": "npm run dev"
    },
    "functions": {
      "port": 5006,
      "host": "0.0.0.0"
    }
  }
}
